{
  "cli": {
    "version": ">= 3.7.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "autoIncrement": true,
      "distribution": "internal",
      "android": {
        "autoIncrement": "versionCode",
        "buildType": "apk"
      },
      "channel": "preview",
      "ios": {
        "image": "latest",
        "autoIncrement": "buildNumber"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "preview2"
    },
    "preview3": {
      "developmentClient": true,
      "channel": "preview3"
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "android": {
        "autoIncrement": "versionCode"
      },
      "ios": {
        "image": "latest",
        "autoIncrement": "buildNumber"
      }
    }
  },
  "submit": {
    "preview": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      },
      "ios": {
        "appleId": "renandeluca01@gmail.com",
        "ascAppId": "1451455075",
        "appleTeamId": "KQ3VDUCQHH"
      }
    }
  }
}
